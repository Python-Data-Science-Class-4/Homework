def word_order(input_str):
    words_list = input_str.split('-')

    sorted_words_list = sorted(words_list)

    new_text = '-'.join(sorted_words_list)

    return new_text

unordered_words = input("Please type your words seperated with '-' : ")

result = word_order(unordered_words)

print("Your Input is:", unordered_words)
print("Sorted result is: ", result)