def unique_list(input_list):
    unique_elements = list(set(input_list))
    return unique_elements

input_str = input("Enter your numbers seperated with comma :  ")
input_list = []

for item in input_str.split(','):
    input_list.append(int(item))

result = unique_list(input_list)

print("Unique Numbers List:  ", result)
